    "import nest_asyncio\n",
    "nest_asyncio.apply()\n",
    "\n",
    "import asyncio\n",
    "from chunks import Chunk\n",
    "\n",
    "chunk = Chunk()\n",
    "query = 'Какой компании ты консультант?'\n",
    "\n",
    "async def main():\n",
    "    result = await chunk.get_answer(query)\n",
    "    print(result)\n",
    "\n",
    "asyncio.run(main())\n"
   ]
